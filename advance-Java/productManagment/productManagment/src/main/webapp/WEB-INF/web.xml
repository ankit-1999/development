<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserLogin</servlet-name>
  	<display-name>UserLogin</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.trainee.advanceJava.controller.UserLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddProduct</servlet-name>
  	<display-name>AddProduct</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.trainee.advanceJava.controller.AddProduct</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserLogout</servlet-name>
  	<display-name>UserLogout</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.trainee.advanceJava.controller.UserLogout</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>DeleteProduct</servlet-name>
  	<display-name>DeleteProduct</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.trainee.advanceJava.controller.DeleteProduct</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetUserPassword</servlet-name>
  	<display-name>GetUserPassword</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.trainee.advanceJava.controller.GetUserPassword</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProduct</servlet-name>
  	<display-name>EditProduct</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.trainee.advanceJava.controller.EditProduct</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>UserLogin</servlet-name>
  	<url-pattern>/loginUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddProduct</servlet-name>
  	<url-pattern>/addProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserLogout</servlet-name>
  	<url-pattern>/logoutUser</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
  	<servlet-name>DeleteProduct</servlet-name>
  	<url-pattern>/DeleteProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetUserPassword</servlet-name>
  	<url-pattern>/getUserPassword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProduct</servlet-name>
  	<url-pattern>/editProduct</url-pattern>
  </servlet-mapping>
 
 
  
</web-app>
